#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <string.h>
#include "linkedlist.h"
#include <sys/types.h>
#include <sys/wait.h>
#include <sys/stat.h>
#include <fcntl.h>




int main(){

  int fd = open("farts.txt", O_WRONLY);
  int *args = {"dogs are cute!", NULL};
  
  if (fd!=0){
    dup2(fd, 1);
  }else{
    puts("ERROR: cannot form out-redirect!");
  }

  execv("farts.txt", args);
  
  return 0;

}
